class Solution {
public:
    int minMoves(vector<int>& nums) {
       int min=nums[0],i=0;
       int result=0;
       for(i=0;i<nums.size();i++)
       {
        if(nums[i]<min)
        {
            min=nums[i];
        }
       } 
       for(i=0;i<nums.size();i++)
       {
        result+=nums[i]-min;
       }
       return result;
    }
};
