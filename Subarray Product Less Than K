class Solution {
public:
    int numSubarrayProductLessThanK(vector<int>& nums, int k) {
        int i=0,j=0;
        int product=1;
        int ans=0;
        int n=nums.size();
        for(int j=0;j<n;j++){
            product*=nums[j];
            while(product>=k &&i<=j){
                product/=nums[i];
                i++;
            }
            ans=ans+j-i+1;
        }
        
        return ans;
    }
};
