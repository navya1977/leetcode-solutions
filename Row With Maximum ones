class Solution {
public:
    vector<int> rowAndMaximumOnes(vector<vector<int>>& mat) {
      int row =0;
      int count =0;
      int maxcout =0;
      for (int i=0; i<mat.size(); i++){
        count =0;
        for (int j=0; j<mat[0].size(); j++){
            if (mat[i][j]==1){
                count++;
            }
        }
        if (count>maxcout){
            maxcout = count;
            row =i;
        }
      }  
      return {row,maxcout};
    }
};
