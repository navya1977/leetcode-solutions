class Solution {
public:
    int findLucky(vector<int>& arr) {
       unordered_map<int,int>mp;
       int maxlen=-1;
       int n=arr.size();
       for(int i=0;i<n;i++){
        mp[arr[i]]++;
       } 
       for(auto p:mp)
       {
        if(p.second==p.first)
        {
            maxlen=max(maxlen,p.first);
        }
       }
      return maxlen;

    }
};
