class Solution {
public:
   void preorder_tr(vector<int>&ans,TreeNode* root) {
        if(root==nullptr)return;
        ans.push_back(root->val);
        preorder_tr(ans,root->left);
        preorder_tr(ans,root->right);
        
    }
    vector<int> preorderTraversal(TreeNode* root) {
    vector<int>ans;
    preorder_tr(ans,root);
    return ans;
    }
};
