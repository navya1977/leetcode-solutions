class Solution {
public:
   void postorder_tr(vector<int>&ans,TreeNode* root) {
        if(root==nullptr)return;
        postorder_tr(ans,root->left);
        postorder_tr(ans,root->right);
        ans.push_back(root->val);
        
    }
    vector<int> postorderTraversal(TreeNode* root) {
    vector<int>ans;
    postorder_tr(ans,root);
    return ans;
    }
};
