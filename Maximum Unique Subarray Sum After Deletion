class Solution {
public:
    int maxSum(vector<int>& nums) {
        unordered_set<int>st(nums.begin(),nums.end());
        int ans=0;
        bool found=false;
        for(auto it:st){
            if(it>0){
                ans+=it;
                found=true;}
            }
            if(found==false) return *max_element(nums.begin(),nums.end());
                   return ans;
    }
};
